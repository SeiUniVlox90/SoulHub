-- Scripts:

local function FFOJIO_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local Msg = script.Parent.Msg
	local Start = script.Parent.Start
	local Main = script.Parent
	
	Msg.Text = "Continue?"
	
	Start.MouseButton1Click:Connect(function()
		Msg.Text = "Loading Gui"
		wait(5)
		Msg.Text = "Destroying In 3"
		wait(1)
		Msg.Text = "Destroying In 2"
		wait(1)
		Msg.Text = "Destroying In 1"
		wait(1)
		loadstring(game:HttpGet((""), true))()
		Main.Visible = false
	end)
end
coroutine.wrap(FFOJIO_fake_script)()
