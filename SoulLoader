local UICorner = Instance.new("UICorner")
local Msg = Instance.new("TextLabel")
local UICorner_2 = Instance.new("UICorner")

Loader.Name = "Loader"
Loader.Parent = game.CoreGui
Loader.ResetOnSpawn = false

Main.Name = "Main"
