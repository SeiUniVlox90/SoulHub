-- Scripts:

local function XLYV_fake_script() -- Char.READ ME SECOND 
	local script = Instance.new('LocalScript', Char)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size150x150 --- change size if needed  (1)
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	local imageLabel = script.Parent
	imageLabel.Image = content
	imageLabel.Size = UDim2.new(0, 60, 0, 60)--- change the 2 150s to the same number you picked for (1)
	
end
coroutine.wrap(XLYV_fake_script)()
local function VLEYOP_fake_script() -- User.read me  
	local script = Instance.new('LocalScript', User)

	script.Parent.Text = " ".. game.Players.LocalPlayer.Name .." "
	
end
coroutine.wrap(VLEYOP_fake_script)()
local function VLGULXO_fake_script() -- Fps.Calculate 
	local script = Instance.new('LocalScript', Fps)

	local FpsCount = 0
	
	game:GetService("RunService").RenderStepped:connect(function()
		FpsCount = FpsCount + 1
	end)
	
	while wait(1) do
		script.Parent.Text = FpsCount .. " FPS"
		FpsCount = 0
	end
	
end
coroutine.wrap(VLGULXO_fake_script)()
local function CUFIUU_fake_script() -- Main.MainHandler 
	local script = Instance.new('LocalScript', Main)

	local Main = script.Parent
	local Destroy = script.Parent.Pages.Home.DestroyUI
	
	Main.Position = UDim2.new(0.5, -250,1.5, -145)
	wait(0.5)
	Main:TweenPosition(UDim2.new(0.5, -250,0.5, -145),"In","Sine",1,false)
	
	Destroy.MouseButton1Click:Connect(function()
		Destroy.Text = "Destroying"
		wait(1.5)
		Main.Visible = false
	end)
	
	Destroy.MouseEnter:Connect(function()
		Destroy.Text = "Destroy?"
		Destroy.BackgroundColor3 = Color3.fromRGB(255, 0, 4)
		wait(0.8)
	end)
	
	Destroy.MouseLeave:Connect(function()
		Destroy.Text = "Destroy UI"
		Destroy.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
	end)
	
end
coroutine.wrap(CUFIUU_fake_script)()
local function UQUPPT_fake_script() -- Main.PageHandler 
	local script = Instance.new('LocalScript', Main)

	local Home = script.Parent.Pages.Home
	local Main = script.Parent.Pages.Main
	local Teleport = script.Parent.Pages.Teleport
	
	local HomeB = script.Parent.Buttons.Home
	local MainB = script.Parent.Buttons.Main
	local TeleportB = script.Parent.Buttons.Teleport
	
	HomeB.MouseButton1Click:Connect(function()
		Home:TweenPosition(UDim2.new(0.016, 0,0.03, 0),"In","Sine",0.5,false)
		Main:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
		Teleport:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
	end)
	
	MainB.MouseButton1Click:Connect(function()
		Main:TweenPosition(UDim2.new(0.016, 0,0.03, 0),"In","Sine",0.5,false)
		Home:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
		Teleport:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
	end)
	
	TeleportB.MouseButton1Click:Connect(function()
		Teleport:TweenPosition(UDim2.new(0.016, 0,0.03, 0),"In","Sine",0.5,false)
		Main:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
		Home:TweenPosition(UDim2.new(0.016, 0,1.03, 0),"In","Sine",0.5,false)
	end)
end
coroutine.wrap(UQUPPT_fake_script)()
local function WDLKWW_fake_script() -- Main.ThemeHandler 
	local script = Instance.new('LocalScript', Main)

	local Brush = script.Parent.brush
	local Close = script.Parent.ThemeF.Close
	local Default = script.Parent.ThemeF.Default
	local Orange = script.Parent.ThemeF.Orange
	local Blue = script.Parent.ThemeF.Blue
	local White = script.Parent.ThemeF.White
	local Logo788 = script.Parent.Logo788
	local ThemeF = script.Parent.ThemeF
	local Main = script.Parent
	
	ThemeF.Position = UDim2.new(0.61, 0,1.014, 0)
	
	Brush.MouseButton1Click:Connect(function()
		ThemeF:TweenPosition(UDim2.new(0.61, 0,0.014, 0),"In","Sine",1,false)
	end)
	
	Close.MouseButton1Click:Connect(function()
		ThemeF:TweenPosition(UDim2.new(0.61, 0,1.014, 0),"In","Sine",1,false)
	end)
	
	Default.MouseButton1Click:Connect(function()
		Brush.ImageColor3 = Color3.fromRGB(255, 255, 255)
		Logo788.TextColor3 = Color3.fromRGB(255, 0, 0)
		Main.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	end)
	
	Orange.MouseButton1Click:Connect(function()
		Brush.ImageColor3 = Color3.fromRGB(255, 255, 255)
		Logo788.TextColor3 = Color3.fromRGB(255, 255, 255)
		Main.BackgroundColor3 = Color3.fromRGB(255, 85, 0)
	end)
	
	Blue.MouseButton1Click:Connect(function()
		Brush.ImageColor3 = Color3.fromRGB(255, 255, 255)
		Logo788.TextColor3 = Color3.fromRGB(255, 255, 255)
		Main.BackgroundColor3 = Color3.fromRGB(0, 0, 127)
	end)
	
	White.MouseButton1Click:Connect(function()
		Brush.ImageColor3 = Color3.fromRGB(0, 0, 0)
		Logo788.TextColor3 = Color3.fromRGB(255, 0, 0)
		Main.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	end)
end
coroutine.wrap(WDLKWW_fake_script)()
