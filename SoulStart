Aura.Name = "Aura"
Aura.Parent = Main_3
Aura.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
Aura.Position = UDim2.new(0.0145228505, 0, 0.331818163, 0)
Aura.Size = UDim2.new(0, 102, 0, 25)
Aura.Font = Enum.Font.GothamBold
Aura.Text = "Kill Aura"
Aura.TextColor3 = Color3.fromRGB(255, 255, 255)
Aura.TextSize = 14.000
Aura.MouseButton1Click:Connect(function()
	 game:GetService("StarterGui"):SetCore("SendNotification", {
Title = "Info:";
Text = "Press E To Kill People Near You!";
})

    plr = game:service'Players'.LocalPlayer
char = plr.Character
mouse = plr:GetMouse()
on = false

mouse.KeyDown:connect(function(key)
  if key == "e" then
      on = true
  end
end)

mouse.KeyUp:connect(function(key)
  if key == "e" then
      on = false
  end
end)

while wait(0.1) do
  for i, b in pairs(game.Players:GetChildren()) do
      if b.Name ~= plr.Name and not b:IsFriendsWith(plr.UserId) and on then
          for i = 1, 10 do
              game.ReplicatedStorage.meleeEvent:FireServer(b)
          end
      end
  end
end


while true do
 wait(0.01)
 for i,v in pairs (game.Players:GetChildren()) do
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[v.Name].Character.HumanoidRootPart.CFrame
   game.ReplicatedStorage.meleeEvent:FireServer(v.Name)
   wait(.8)
 end
end
end)

